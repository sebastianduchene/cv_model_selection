   "source": [
    "for(s in dir(pattern = 'fasta')){\n",
    "    dat <- read.dna(s, 'fasta')\n",
    "    dat_s <- dat[, 1:(ncol(dat) / 2)]\n",
    "    write.dna(dat_s, file = gsub('.fasta', '_subset.fasta', s), format = 'fasta', nbcol = -1, colsep = '')\n",
    "}"
   ]
